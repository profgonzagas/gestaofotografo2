{
  "name": "gestaofotografo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production  --base-href=/gestaofotografo2/",
    "build-prod": "ng build --configuration=production --base-href=\"/gestaofotografo2/\"",
    "ghpages": "angular-cli-ghpages --dir=dist/gestaofotografo/",
    "deploy": "npm run build & npm run ghpages",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start-json-server": "json-server --watch db.json",
    "deploy:ghdocs": "ng build --configuration=production --output-path docs --base-href /gestaofotografo2/ & angular-cli-ghpages --dir=docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.7",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.7",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^17.0.7",
    "@angular/forms": "^17.0.7",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^17.0.7",
    "@angular/router": "^17.0.7",
    "angular-cli-ghpages": "^1.0.7",
    "json-server": "^0.17.4",
    "material-design-icons": "^3.0.1",
    "material-icons": "^1.13.12",
    "materialize-css": "^1.0.0",
    "ngx-mask": "^17.0.4",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.7",
    "@types/jasmine": "^5.1.4",
    "@types/materialize-css": "^1.0.14",
    "jasmine-core": "^5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.2.2"
  }
}